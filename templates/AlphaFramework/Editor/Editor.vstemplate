<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>AlphaEditor</Name>
    <Description>An IDE-like Editor for your game based on the OmegaEngine and AlphaFramework.
You can use it to create GUI dialogs, maps, particle systems, etc..</Description>
    <ProjectType>CSharp</ProjectType>
	<ProjectSubType>OmegaEngine</ProjectSubType>
    <SortOrder>1001</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>Editor</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <PromptForSaveOnCreation>true</PromptForSaveOnCreation>
    <Icon>..\AlphaFramework.ico</Icon>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="Editor.csproj" File="Editor.csproj" ReplaceParameters="false">
      <ProjectItem ReplaceParameters="false" TargetFileName="MainForm.cs">MainForm.cs</ProjectItem>
      <ProjectItem ReplaceParameters="false" TargetFileName="MainForm.Designer.cs">MainForm.Designer.cs</ProjectItem>
      <ProjectItem ReplaceParameters="false" TargetFileName="Program.cs">Program.cs</ProjectItem>
      <ProjectItem ReplaceParameters="false" TargetFileName="RenderPanel.cs">RenderPanel.cs</ProjectItem>
      <Folder Name="Properties" TargetFolderName="Properties">
        <ProjectItem ReplaceParameters="true" TargetFileName="AssemblyInfo.cs">AssemblyInfo.cs</ProjectItem>
        <ProjectItem ReplaceParameters="false" TargetFileName="Settings.settings">Settings.settings</ProjectItem>
        <ProjectItem ReplaceParameters="false" TargetFileName="Settings.Designer.cs">Settings.Designer.cs</ProjectItem>
      </Folder>
    </Project>
  </TemplateContent>
  <WizardExtension>
    <Assembly>NuGet.VisualStudio.Interop, Version=1.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a</Assembly>
    <FullClassName>NuGet.VisualStudio.TemplateWizard</FullClassName>
  </WizardExtension>
  <WizardData>
    <packages repository="extension" repositoryId="OmegaEngine_Templates">
      <package id="ICSharpCode.SharpZipLib.Patched" version="0.86.3" />
      <package id="LinqBridge" version="1.3.0" />
      <package id="SlimDX" version="4.0.13.44" />
      <package id="NanoByte.Common" version="1.1.1" />
      <package id="NanoByte.Common.WinForms" version="1.1.1" />
      <package id="NanoByte.Common.SlimDX" version="1.1.1" />
      <package id="OmegaEngine.Backend" version="0.9.0" />
      <package id="OmegaGUI" version="0.9.0" />
      <package id="AlphaFramework.World" version="0.9.0" />
      <package id="AlphaFramework.Presentation" version="0.9.0" />
      <package id="AlphaEditor" version="0.9.0" />
    </packages>
  </WizardData>
</VSTemplate>
